{
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "quoteProps": "consistent",
  "printWidth": 120,
  "importOrder": ["<THIRD_PARTY_MODULES>", "^~/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrderParserPlugins": ["typescript", "classProperties", "decorators-legacy"]
}
